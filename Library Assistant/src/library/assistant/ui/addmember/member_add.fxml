<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="332.0" prefWidth="486.0" stylesheets="@../../../../resources/dark-theme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.assistant.ui.addmember.MemberAddController">
